{
  "manifest_version": 3,
  "name": "GV - Calls Queue",
  "version": "1.0",
  "description": "PoC of Side Panel Chrome Extension with React + Vite",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    },
    "default_title": "Open GV - Calls Queue"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel", "storage", "tabs", "notifications"],
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
